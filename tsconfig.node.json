{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "typeRoots": ["./node_modules/@types"],
    "types": ["node", "papaparse"]
  },
  "exclude": ["node_modules", "dist"]
}
